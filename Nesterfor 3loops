outerloop =0
innerloop_k=0
innerloop_j=0
print( "i j k") 
for i in range(4):
         outerloop+=1
         for j in range(4):
                  innerloop_j+=1
                  for k in range(4):
                         innerloop_k+=1
                         print(i, j, k)
print("---loops completed----")
print("outerloop executed for %d times"%outerloop) 
print("innerloop executed for %d times"%innerloop_k) 

